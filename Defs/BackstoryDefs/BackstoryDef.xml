        <amount>4</amount>
      </li>
    </skillGains>
  </AlienRace.BackstoryDef>
  <AlienRace.BackstoryDef ParentName="TaurusAdultBase">
    <defName>TaurusLoner</defName>
    <title>Loner</title>
    <titleShort>Loner</titleShort>
